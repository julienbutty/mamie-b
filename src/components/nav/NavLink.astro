---
import '@fontsource/baloo-2'
interface Props {
  link: string;
}

const { link } = Astro.props;
---

<a
  href="#"
  class="text-my-primary hover:bg-my-secondary whitespace-nowrap rounded-md px-3 py-2 text-xl font-medium hover:text-white"
  >{link}</a
>
<style>
 a {
  font-family: 'Baloo 2', cursive;
 } 
</style>
